          { "type": "Lab3.Calculator.ICalculator", "key": "catCalc" }
        ]
      },
      {
        "type": "Lab3.Calculator.PlusCalc",
        "services": [
          { "type": "Lab3.Calculator.ICalculator", "key": "plusCalc" }
        ]
      },
      {
        "type": "Lab3.Calculator.StateCalc",
        "services": [
          { "type": "Lab3.Calculator.ICalculator", "key": "stateCalc" }
